		<table domainObjectName="BabyRequireItem" tableName="T_BABY_REQUIRE_ITEMS" />
		<table domainObjectName="BabyRequire" tableName="T_BABY_REQUIRE" />
		<table domainObjectName="BabyInfo" tableName="T_BABY_INFO" />
		<table domainObjectName="BabyUserInfo" tableName="T_BABY_USER_INFO" />
		<table domainObjectName="BabyEvaluation" tableName="t_baby_evaluation" />
		<table domainObjectName="BabyPayment" tableName="T_BABY_PAYMENT_RECORD" />
		<table domainObjectName="BabyInvite" tableName="T_BABY_INVITE" /> 
		<table domainObjectName="BabyTimePrice" tableName="T_BABY_TIME_PRICE" /> -->

